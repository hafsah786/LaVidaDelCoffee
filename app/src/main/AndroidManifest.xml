<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.smithjterm.lavidadelcoffee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LatteActivity" />
        <activity android:name=".CappuccinoActivity" />
        <activity android:name=".MochaActivity" />
        <activity android:name=".AmericanoActivity" />
        <activity android:name=".EspressoActivity" />
        <activity android:name=".MacchiatoActivity" />
        <activity android:name=".AmericanoLocationActivity" />
        <activity android:name=".CappuccinoLocationActivity" />
        <activity android:name=".EspressoLocationActivity" />
        <activity android:name=".LatteLocationActivity" />
        <activity android:name=".MacchiatoLocationActivity" />
        <activity android:name=".MochaLocationActivity" />
        <activity android:name=".consumedActivity" />
        <activity android:name=".NoRemainingCoffeeActivity" />
        <activity android:name=".preferenceActivity"></activity>
    </application>

</manifest>